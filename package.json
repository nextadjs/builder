{
  "name": "@nextad/builder",
  "version": "0.1.6",
  "description": "A fluent builder library for OpenRTB 2.6 bid requests and responses with TypeScript support",
  "scripts": {
    "test": "vitest",
    "build": "tsup"
  },
  "keywords": [
    "OpenRTB",
    "builder",
    "BidRequest",
    "BidResponse"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./v26": {
        "import": "./dist/2.6/index.mjs",
        "require": "./dist/2.6/index.js",
        "types": "./dist/2.6/index.d.ts"
      }
    }
  },
  "author": "Kai Miyamoto",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "uuid": "^11.0.5"
  },
  "peerDependencies": {
    "iab-openrtb": "^0.3.10"
  },
  "files": [
    "dist/",
    "LICENCE",
    "README.md"
  ],
  "homepage": "https://github.com/nextadjs/builder.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextadjs/builder.git"
  }
}
